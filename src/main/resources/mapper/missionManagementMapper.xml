<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.feima.project_management.dao.MissionManagementMapper">
    <insert id="missionInsert" parameterType="com.feima.project_management.model.Mission" >
      INSERT into mission VALUES (#{Mission_Id},#{Mission_Name},#{Mission_Details},#{Mission_Role},#{Mission_Date_Begin},#{Mission_Date_Over})
    </insert>
    <select id="missionselect">
      SELECT * from mission limit 15
    </select>
    <delete id="missionDeleteByPrimaryKey">
      DELETE FROM misson WHERE Mission_Id = ?
    </delete>
</mapper>